<!DOCTYPE html>
<html lang="{% block language %}{% endblock %}">
<head>
    <meta charset="UTF-8">
    <title>{% block title %}Welcome!{% endblock %}</title>

    {% block metatags %}{% endblock %}

    {% block stylesheets %}
        <link href="{{ asset('css/normalize.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/skeleton.css') }}" rel="stylesheet"/>
        <link href="{{ asset('css/rss_feed.css') }}" rel="stylesheet"/>
        <link rel="stylesheet" type="text/css" href="https://cdn.jsdelivr.net/gh/greghub/green-audio-player/dist/css/green-audio-player.min.css"/>
    {% endblock %}

    {% block javascripts %}
        <script src="https://unpkg.com/htmx.org@1.3.1"
                integrity="sha384-fIzHaKYVrEFxI4/dWpMzf9tlIUPU0HmW8w/8s4nQHvZmGQhXhnEM6aenC6QFkd7e"
                crossorigin="anonymous">
        </script>
        <script src="https://cdn.jsdelivr.net/gh/greghub/green-audio-player/dist/js/green-audio-player.min.js">
        </script>
        <script src="{{ asset('js/rss_feed.js') }}"
                async>
        </script>
    {% endblock %}
    {% block favicon %}{% endblock %}
</head>
<body>
{% block body %}{% endblock %}
</body>
</html>

